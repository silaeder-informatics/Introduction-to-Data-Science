   "source": [
    "from math import sqrt\n",
    "arr = [-1, -1, -1, 2, 1, 1, 1]\n",
    "mean = sum(arr) / len(arr)\n",
    "\n",
    "summ = [(elem - mean) ** 2 for elem in arr]\n",
    "sqrt(sum(summ) / (len(arr) - 1))"
   ]
